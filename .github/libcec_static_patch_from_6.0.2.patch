-    ssize_t iBytesRead(0);
-    *iError = 0;
-
-    if (socket == INVALID_SOCKET ||
-        socket == SOCKET_ERROR ||
-        len != (int)len)
-    {
-      *iError = EINVAL;
-      return -1;
-    }
-
-    if (iTimeoutMs > 0)

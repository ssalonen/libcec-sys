-struct StdStringLessNoCaseW
-  : std::binary_function<CStdStringW, CStdStringW, bool>
-{
-  inline
-  bool operator()(const CStdStringW& sLeft, const CStdStringW& sRight) const
-  { return ssicmp(sLeft.c_str(), sRight.c_str()) < 0; }

-      }
-
-      if (iReadResult < 0)
-      {
-        if (iSocketError == EAGAIN && iTimeoutMs > 0)
-          continue;
-        *iError = iSocketError;
-        return (iBytesRead > 0) ? iBytesRead : -iSocketError;

-  int Replace(PCMYSTR szOld, PCMYSTR szNew)
-  {
-    int nReplaced    = 0;
-    MYSIZE nIdx      = 0;
-    MYSIZE nOldLen    = sslen(szOld);
-
-    if ( 0 != nOldLen )
